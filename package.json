  "license": "ISC",
  "bugs": {
    "url": "https://github.com/yusefnapora/js-ipfs-pinning-service-client/issues"
  },
  "homepage": "https://github.com/yusefnapora/js-ipfs-pinning-service-client#readme",
  "devDependencies": {
    "@openapitools/openapi-generator-cli": "^2.1.26",
    "aegir": "^32.1.0"
  }
}
